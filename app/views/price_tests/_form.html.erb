<%= form_for price_test do |f| %>
  <%= f.hidden_field(:product_id, value: product.id) %>
  <div>
    <%= f.label(:percent_increase)%>
    <%= f.number_field(:percent_increase) %>
  </div>
  <div>
    <%= f.label(:percent_decrease)%>
    <%= f.number_field(:percent_decrease) %>
  </div>
  <div>
    <%= f.label(:ending_digits)%>
    <%= select("price_test", "ending_digits", [0.99, 0.95, 0.50, 0.25, 0] ) %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>